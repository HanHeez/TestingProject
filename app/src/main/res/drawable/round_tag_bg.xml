<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/length_1" />
            <stroke android:width="@dimen/length_1px" android:color="@color/d4b185" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/length_1" />
            <stroke android:width="@dimen/length_1px" android:color="@color/color_000000" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>