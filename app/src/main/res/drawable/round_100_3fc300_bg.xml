    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="100.0dip" />
            <stroke android:width="0.0px" android:color="#ff3fc300" />
            <solid android:color="#ff3fc300" />
        </shape>
    </item>
</selector>